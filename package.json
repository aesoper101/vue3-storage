{
  "name": "vue3-storage",
  "version": "0.1.5",
  "description": "Vue3 plugin for work with local storage, session storage and websql from Vue context, inspired by tarojs and vue-ls.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aesoper101/vue3-storage.git"
  },
  "keywords": [
    "vue3",
    "vue3-storage",
    "storage",
    "localstorage",
    "sessionstorage",
    "promise",
    "vue-ls",
    "synchronous storage",
    "sync storage",
    "asynchronous storage",
    "async storage"
  ],
  "author": "aesoper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aesoper101/vue3-storage/issues"
  },
  "homepage": "https://github.com/aesoper101/vue3-storage#readme",
  "private": false,
  "main": "dist/vue3-storage.cjs.js",
  "browser": "dist/vue3-storage.esm.js",
  "module": "dist/vue3-storage.esm.js",
  "unpkg": "dist/vue3-storage.min.js",
  "types": "dist/packages/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:lib": "yarn clean:dist && yarn build:cjs && yarn build:es && yarn build:unpkg && rimraf dist/src && rimraf dist/tests && yarn build:version",
    "build:cjs": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "build:version": "node build/gen-version.js",
    "clean:dist": "rimraf dist",
    "publish": "npm publish --registry=http://registry.npmjs.org",
    "unpublish": "npm unpublish vue3-storage --force --registry=http://registry.npmjs.org"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.0-0"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-auto-install": "^2.1.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "fs": "^0.0.1-security",
    "minimist": "^1.2.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-0"
  }
}
